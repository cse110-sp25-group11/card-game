{
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "babel-jest": "^30.0.0-beta.3",
    "codacy-coverage": "^3.4.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.0-beta.3",
    "jest-puppeteer": "^11.0.0",
    "jsdoc": "^4.0.4",
    "minami": "^1.2.3",
    "puppeteer": "^24.10.0",
    "taffydb": "^2.7.3"
  },
  "scripts": {
    "test:unit": "node --experimental-vm-modules node_modules/jest/bin/jest.js __tests__/ --coverage --env=jsdom",
    "test:e2e": "NODE_OPTIONS='--experimental-vm-modules' node_modules/jest/bin/jest.js _e2e__/ --coverage"
  },
  "type": "module",
  "jest": {
    "preset": "jest-puppeteer",
    "transform": {},
    "verbose": true,
    "testMatch": [
      "**/__tests__/*.test.js",
      "**/_e2e__/*.test.js"
    ]
  },
  "prettier": {
    "tabWidth": 4,
    "useTabs": false
  }
}
